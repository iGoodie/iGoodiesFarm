package igoodie.goodiesfarm.items;

import igoodie.goodiesfarm.tabs.GoodiesFarmTab;
import cpw.mods.fml.relauncher.Side;
import cpw.mods.fml.relauncher.SideOnly;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.ItemFood;
import net.minecraft.item.ItemSword;

public class ItemRawMutton extends ItemSword
{
	public ItemRawMutton() 
	{
		//super(1, 0.1f, true);
		super();
		
		this.setCreativeTab(GoodiesFarmTab.tabInstance);
		this.setUnlocalizedName("itemRawMutton");
		this.setMaxStackSize(64);
		this.setTextureName("itemRawMutton");
		
	}
	
	//@SideOnly(Side.CLIENT)
	
}
