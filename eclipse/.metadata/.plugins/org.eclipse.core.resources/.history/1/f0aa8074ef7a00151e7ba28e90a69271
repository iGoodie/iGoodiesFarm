	}
	
	private int findGround(World world, int x, int z)
	{
		int yNeeded;
		for(int y=world.getActualHeight(); y>=0; y--)
		{
			if(world.isAirBlock(x, y, z) && (world.getBlock(x, y-1, z) instanceof BlockDirt));
		}
		return yNeeded;
	}

